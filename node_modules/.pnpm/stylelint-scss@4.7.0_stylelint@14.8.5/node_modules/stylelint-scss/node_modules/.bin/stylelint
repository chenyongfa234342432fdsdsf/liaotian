#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../stylelint@14.8.5/node_modules/stylelint/bin/stylelint.js" "$@"
else
  exec node  "$basedir/../../../../../stylelint@14.8.5/node_modules/stylelint/bin/stylelint.js" "$@"
fi
