{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devPath": "http://localhost:4000",
    "distDir": "../dist/client"
  },
  "package": {
    "productName": "VanChat",
    "version": "1.0.5"
  },
  "tauri": {
    "allowlist": {
      "shell": {
        "all": true,
        "execute": true,
        "sidecar": true,
        "open": true
      },
      "window": {
        "create": true
      },
      "os": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "SocialNetworking",
      "copyright": "",
      "appimage": {
        "bundleMediaFramework": true
      },
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
      "identifier": "talk-blink.test",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false,
      "dialog": true,
      "endpoints": [
        "https://bff.tbkplus.com/updater/info?bid=1&target={{target}}&version={{current_version}}&arch={{arch}}"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDdENzNGRkM1OTExMzMyMDcKUldRSE1oT1J4Zjl6Zlh5VWRNdzhYVzdTNE8yN1hDNnFUMEZOczcwdTFkU1NEcHpGaC9pNE9Ub0wK"
    },
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "height": 900,
        "minWidth": 1072,
        "minHeight": 844,
        "resizable": true,
        "title": "",
        "width": 1440,
        "hiddenTitle": true,
        "tabbingIdentifier": "main"
      }
    ]
  }
}
